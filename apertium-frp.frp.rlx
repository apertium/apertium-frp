SELECT Imper IF (-1 BOS OR Sent OR (lpar)) (0 ("<Va>")) ;
REMOVE Ind OR Subjunc IF (-1 BOS OR Sent OR (lpar)) (0 Imper) ;		# Salye de ton payis

